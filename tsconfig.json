{
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "sourceMap": true,
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "node",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "types": ["jest"],
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ],
